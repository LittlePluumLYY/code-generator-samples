<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.generator.mapper.BotpyDao">

    <resultMap id="botpyResultMap" type="com.generator.model.Botpy">
        <id column="id" property="id"/>
        <result column="company" property="company"/>
        <result column="name" property="name"/>
        <result column="business_line" property="businessLine"/>
        <result column="position" property="position"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

  	<sql id="Where_Clause">
      <where>
        <if test="id != null">
            AND id = #{id}
        </if>
        <if test="company != null and company != '' ">
            AND company = #{company}
        </if>
        <if test="name != null and name != '' ">
            AND name = #{name}
        </if>
        <if test="businessLine != null and businessLine != '' ">
            AND business_line = #{businessLine}
        </if>
        <if test="position != null and position != '' ">
            AND position = #{position}
        </if>
        <if test="createTime != null">
            AND create_time = #{createTime}
        </if>
        <if test="updateTime != null">
            AND update_time = #{updateTime}
        </if>
      </where>
  	</sql>

    <sql id="Base_Column_List">
        id,
        company,
        name,
        business_line,
        position,
        create_time,
        update_time
    </sql>

    <!-- 新增数据 -->
	<insert id="insertBotpy" parameterType="com.generator.model.Botpy">
        INSERT INTO management.botpy (
            <trim suffixOverrides=",">
                <if test="id != null">id,</if>
                <if test="company != null">company,</if>
                <if test="name != null">name,</if>
                <if test="businessLine != null">business_line,</if>
                <if test="position != null">position,</if>
                <if test="createTime != null">create_time,</if>
                <if test="updateTime != null">update_time,</if>
            </trim>
        ) VALUES (
            <trim suffixOverrides=",">
                <if test="id != null">#{id,jdbcType=INTEGER},</if>
                <if test="company != null">#{company,jdbcType=VARCHAR},</if>
                <if test="name != null">#{name,jdbcType=VARCHAR},</if>
                <if test="businessLine != null">#{businessLine,jdbcType=VARCHAR},</if>
                <if test="position != null">#{position,jdbcType=VARCHAR},</if>
                <if test="createTime != null">#{createTime,jdbcType=timestamp},</if>
                <if test="updateTime != null">#{updateTime,jdbcType=timestamp},</if>
            </trim>
        )
	</insert>

    <!-- 根据主键删除数据 -->
	<delete id="deleteBotpyByPrimaryKey" parameterType="java.lang.Integer">
        DELETE FROM management.botpy
        WHERE id = #{id} 
	</delete>

    <!-- 根据传入参数删除数据 -->
	<delete id="deleteBotpyByWhere" parameterType="com.generator.model.Botpy">
        DELETE FROM management.botpy
        <include refid="Where_Clause" />
	</delete>

    <!-- 根据主键更新数据 -->
    <update id="updateBotpyByPrimaryKey" parameterType="com.generator.model.Botpy">
		UPDATE management.botpy
		<set>
			<if test="company != null">company = #{company},</if>
			<if test="name != null">name = #{name},</if>
			<if test="businessLine != null">business_line = #{businessLine},</if>
			<if test="position != null">position = #{position},</if>
			<if test="createTime != null">create_time = #{createTime},</if>
			<if test="updateTime != null">update_time = #{updateTime},</if>
		</set>
		WHERE
		 id = #{id}
	</update>

	<!-- 根据主键查询数据  -->
	<select id="queryBotpyByPrimaryKey" resultMap="botpyResultMap" parameterType="java.lang.Integer">
        SELECT
        <include refid="Base_Column_List" />
  	     FROM management.botpy
        WHERE
            id = #{id}
    </select>

	<!-- 根据传入参数查询数据列表  -->
	<select id="queryBotpyListByWhere" resultMap="botpyResultMap" parameterType="com.generator.model.Botpy">
        SELECT
        <include refid="Base_Column_List" />
  	     FROM management.botpy
        <include refid="Where_Clause" />
    </select>

	<!-- 统计符合条件的数据数量 -->
	<select id="countByWhere" resultType="int" parameterType="com.generator.model.Botpy">
		SELECT COUNT(*) FROM management.botpy
		<include refid="Where_Clause" />
	</select>

    <!-- 批量插入数据 -->
	<insert id="batchInsertBotpy" parameterType="java.util.List">
        INSERT INTO management.botpy (
        <trim suffixOverrides=",">
				<if test="null != list[0].id">id,</if>
				<if test="null != list[0].company">company,</if>
				<if test="null != list[0].name">name,</if>
				<if test="null != list[0].businessLine">business_line,</if>
				<if test="null != list[0].position">position,</if>
				<if test="null != list[0].createTime">create_time,</if>
				<if test="null != list[0].updateTime">update_time,</if>
        </trim>
        ) VALUES
        <foreach collection="list" item="item" index= "index" separator =",">
            (
            <trim suffixOverrides=",">
				<if test="null != item.id">#{item.id,jdbcType=INTEGER},</if>
				<if test="null != item.company">#{item.company,jdbcType=VARCHAR},</if>
				<if test="null != item.name">#{item.name,jdbcType=VARCHAR},</if>
				<if test="null != item.businessLine">#{item.businessLine,jdbcType=VARCHAR},</if>
				<if test="null != item.position">#{item.position,jdbcType=VARCHAR},</if>
				<if test="null != item.createTime">#{item.createTime,jdbcType=timestamp},</if>
				<if test="null != item.updateTime">#{item.updateTime,jdbcType=timestamp},</if>
            </trim>
            )
        </foreach>
    </insert>
</mapper>