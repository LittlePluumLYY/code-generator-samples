<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.generator.mapper.ManagementPersonInformationDao">

    <resultMap id="managementPersonInformationResultMap" type="com.generator.model.ManagementPersonInformation">
        <id column="id" property="id"/>
        <result column="cert_no" property="certNo"/>
        <result column="real_name" property="realName"/>
        <result column="organization_name" property="organizationName"/>
        <result column="job_number" property="jobNumber"/>
        <result column="phone" property="phone"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

  	<sql id="Where_Clause">
      <where>
        <if test="id != null">
            AND id = #{id}
        </if>
        <if test="certNo != null and certNo != '' ">
            AND cert_no = #{certNo}
        </if>
        <if test="realName != null and realName != '' ">
            AND real_name = #{realName}
        </if>
        <if test="organizationName != null and organizationName != '' ">
            AND organization_name = #{organizationName}
        </if>
        <if test="jobNumber != null and jobNumber != '' ">
            AND job_number = #{jobNumber}
        </if>
        <if test="phone != null and phone != '' ">
            AND phone = #{phone}
        </if>
        <if test="createTime != null">
            AND create_time = #{createTime}
        </if>
        <if test="updateTime != null">
            AND update_time = #{updateTime}
        </if>
      </where>
  	</sql>

    <sql id="Base_Column_List">
        id,
        cert_no,
        real_name,
        organization_name,
        job_number,
        phone,
        create_time,
        update_time
    </sql>

    <!-- 新增数据 -->
	<insert id="insertManagementPersonInformation" parameterType="com.generator.model.ManagementPersonInformation">
        INSERT INTO management.management_person_information (
            <trim suffixOverrides=",">
                <if test="id != null">id,</if>
                <if test="certNo != null">cert_no,</if>
                <if test="realName != null">real_name,</if>
                <if test="organizationName != null">organization_name,</if>
                <if test="jobNumber != null">job_number,</if>
                <if test="phone != null">phone,</if>
                <if test="createTime != null">create_time,</if>
                <if test="updateTime != null">update_time,</if>
            </trim>
        ) VALUES (
            <trim suffixOverrides=",">
                <if test="id != null">#{id,jdbcType=bigint},</if>
                <if test="certNo != null">#{certNo,jdbcType=VARCHAR},</if>
                <if test="realName != null">#{realName,jdbcType=VARCHAR},</if>
                <if test="organizationName != null">#{organizationName,jdbcType=VARCHAR},</if>
                <if test="jobNumber != null">#{jobNumber,jdbcType=VARCHAR},</if>
                <if test="phone != null">#{phone,jdbcType=VARCHAR},</if>
                <if test="createTime != null">#{createTime,jdbcType=timestamp},</if>
                <if test="updateTime != null">#{updateTime,jdbcType=timestamp},</if>
            </trim>
        )
	</insert>

    <!-- 根据主键删除数据 -->
	<delete id="deleteManagementPersonInformationByPrimaryKey" parameterType="long">
        DELETE FROM management.management_person_information
        WHERE id = #{id} 
	</delete>

    <!-- 根据传入参数删除数据 -->
	<delete id="deleteManagementPersonInformationByWhere" parameterType="com.generator.model.ManagementPersonInformation">
        DELETE FROM management.management_person_information
        <include refid="Where_Clause" />
	</delete>

    <!-- 根据主键更新数据 -->
    <update id="updateManagementPersonInformationByPrimaryKey" parameterType="com.generator.model.ManagementPersonInformation">
		UPDATE management.management_person_information
		<set>
			<if test="certNo != null">cert_no = #{certNo},</if>
			<if test="realName != null">real_name = #{realName},</if>
			<if test="organizationName != null">organization_name = #{organizationName},</if>
			<if test="jobNumber != null">job_number = #{jobNumber},</if>
			<if test="phone != null">phone = #{phone},</if>
			<if test="createTime != null">create_time = #{createTime},</if>
			<if test="updateTime != null">update_time = #{updateTime},</if>
		</set>
		WHERE
		 id = #{id}
	</update>

	<!-- 根据主键查询数据  -->
	<select id="queryManagementPersonInformationByPrimaryKey" resultMap="managementPersonInformationResultMap" parameterType="long">
        SELECT
        <include refid="Base_Column_List" />
  	     FROM management.management_person_information
        WHERE
            id = #{id}
    </select>

	<!-- 根据传入参数查询数据列表  -->
	<select id="queryManagementPersonInformationListByWhere" resultMap="managementPersonInformationResultMap" parameterType="com.generator.model.ManagementPersonInformation">
        SELECT
        <include refid="Base_Column_List" />
  	     FROM management.management_person_information
        <include refid="Where_Clause" />
    </select>

	<!-- 统计符合条件的数据数量 -->
	<select id="countByWhere" resultType="int" parameterType="com.generator.model.ManagementPersonInformation">
		SELECT COUNT(*) FROM management.management_person_information
		<include refid="Where_Clause" />
	</select>

    <!-- 批量插入数据 -->
	<insert id="batchInsertManagementPersonInformation" parameterType="java.util.List">
        INSERT INTO management.management_person_information (
        <trim suffixOverrides=",">
				<if test="null != list[0].id">id,</if>
				<if test="null != list[0].certNo">cert_no,</if>
				<if test="null != list[0].realName">real_name,</if>
				<if test="null != list[0].organizationName">organization_name,</if>
				<if test="null != list[0].jobNumber">job_number,</if>
				<if test="null != list[0].phone">phone,</if>
				<if test="null != list[0].createTime">create_time,</if>
				<if test="null != list[0].updateTime">update_time,</if>
        </trim>
        ) VALUES
        <foreach collection="list" item="item" index= "index" separator =",">
            (
            <trim suffixOverrides=",">
				<if test="null != item.id">#{item.id,jdbcType=bigint},</if>
				<if test="null != item.certNo">#{item.certNo,jdbcType=VARCHAR},</if>
				<if test="null != item.realName">#{item.realName,jdbcType=VARCHAR},</if>
				<if test="null != item.organizationName">#{item.organizationName,jdbcType=VARCHAR},</if>
				<if test="null != item.jobNumber">#{item.jobNumber,jdbcType=VARCHAR},</if>
				<if test="null != item.phone">#{item.phone,jdbcType=VARCHAR},</if>
				<if test="null != item.createTime">#{item.createTime,jdbcType=timestamp},</if>
				<if test="null != item.updateTime">#{item.updateTime,jdbcType=timestamp},</if>
            </trim>
            )
        </foreach>
    </insert>
</mapper>